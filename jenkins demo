pipeline {
    agent any
    stages {
        stage("test") {
            steps {
                echo 'testing application'
            }

      }
       stage("run") {
            steps {
                echo 'running application'
            }
        }
         stage("commit") {
            steps {
                echo 'commit all application'
           }
        }
         stage("production") {
            steps {
                echo 'production appilication'
            }
        }
    }
}

